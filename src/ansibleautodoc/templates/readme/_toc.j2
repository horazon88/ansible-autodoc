{#      Playbooks      #}
{% if r.has_items("playbook", "playbook") %}
* [Playbooks](#playbooks)
{% for _obj in r.get_type("playbook", "playbook") %}
  {% for _item in _obj.keys() %}
    {% set play = _obj.get(_item) %}
  - [{{ play.key}}](#playbook_{{play.file}})
  {% endfor %}
{% endfor %}
{% endif %}
